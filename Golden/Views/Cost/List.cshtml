@model StaticCostRepository
 
@{
    ViewBag.Title = "Cost List";
}
<table class="table table-dark">
    <thead>
    <tr>
        <th>Id</th>
        <th>Amount</th>
        <th>Category</th>
        <th>Comment</th>
        <th colspan="3" class="text-center">Actions</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var cost in Model.costs)
    {
        <partial name="costPartialView" model="cost" />
    }
 
    </tbody>
</table>